    {
        "type": "message",
        "text": "This query is admittedly pretty nasty:\n\nSELECT card, types, count, sum, count\/sum as perc FROM (SELECT card, COUNT(*) FROM cards GROUP BY card) bb JOIN (SELECT name, types, SUM(count) FROM (SELECT name, types, aa.coloridentity, aa.count FROM cardref JOIN (SELECT coloridentity, COUNT(*) FROM decks GROUP BY coloridentity) aa ON cardref.coloridentity &lt;@ aa.coloridentity AND array[‘R’]::varchar[] = cardref.coloridentity) bb GROUP BY name, types) cc ON bb.card = cc.name WHERE ‘Land’!=ALL(types) ORDER BY perc DESC LIMIT 100;",
        "user": "U286Q175Y",

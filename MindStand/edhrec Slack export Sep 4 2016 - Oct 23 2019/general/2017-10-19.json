        "user": "U4V5EHQ2G",
        "ts": "1508437168.000240",
        "team": "T286HM2F3",
        "user_team": "T286HM2F3",
        "source_team": "T286HM2F3",
        "user_profile": {
            "avatar_hash": "cb0f3664c039",

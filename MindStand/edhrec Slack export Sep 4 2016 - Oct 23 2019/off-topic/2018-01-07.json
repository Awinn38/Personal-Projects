    },
    {
        "type": "message",
        "text": "That would be... odd",
        "user": "U3M60281W",
        "ts": "1515334089.000024",
        "team": "T286HM2F3",
        "user_team": "T286HM2F3",
        "source_team": "T286HM2F3",
        "user_profile": {
            "avatar_hash": "358d01a82620",
            "image_72": "https:\/\/avatars.slack-edge.com\/2017-12-17\/287955271268_358d01a826201ec4b686_72.jpg",
